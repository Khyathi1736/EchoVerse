<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="icon" href="./images/logo.png" type="image/png">
  <title>EchoVerse</title>
  <link rel="stylesheet" href="./css/partials.css">
  <link rel="stylesheet" href="/css/contact.css">
  <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
</head>

<body>

  <%- include("./partials/header.ejs") %>
    <div class="bodyContent">
      <section class="c-container">
        <form id="contact-form" action="/post-issue" class="c-flex" method="post">
          <h1 class="c-flex-item">Contact Us</h1>
          <input class="c-flex-item c-input" placeholder="your email" type="text" name="password" id="c-email">
          <input class="c-flex-item c-input" placeholder="describe your issue here" type="text" name="email"
            id="c-issue">
          <input class="c-flex-item c-input" type="submit" value="submit" id="c-submit">
        </form>
      </section>
    </div>
    <%- include("./partials/footer.ejs") %>

      <script src="./js/partials.js"></script>
      <script src="/js/contact.js"></script>
</body>

</html>